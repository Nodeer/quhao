<?xml version="1.0" encoding="UTF-8"?>
<!--	拿号成功之后显示的页面 		-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff"
    android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/getNumberLayout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/common_top_bar_bg_blue"
            android:gravity="center_vertical" >

            <Button
                android:id="@+id/back_btn"
                android:layout_width="60dip"
            	android:layout_height="35dip"
                android:text="@string/btn_back"
	            android:background="@drawable/btn_back_background"
	            android:gravity="center|center_vertical"
	            android:textColor="#ff000000"
	            android:textSize="14.0sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="@string/get_number"
                android:textAppearance="?android:textAppearanceMedium"
                android:textColor="#000000" />
        </RelativeLayout>

       <LinearLayout
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:orientation="vertical"
           android:background="@drawable/tj" >
           <LinearLayout
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:paddingTop="10dip"
               android:baselineAligned="false"
               android:gravity="center_vertical"
               android:orientation="horizontal">
	           <LinearLayout
	               android:layout_width="0dip"
	               android:layout_height="50dip"
	               android:layout_weight="1.0"
	               android:orientation="horizontal"
	               android:layout_gravity="center_vertical"
	               android:paddingLeft="10.0dip" >
	
	               <TextView
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:layout_gravity="center_vertical"
	                   android:text="@string/seat_number"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	
	               <TextView
	                   android:id="@+id/seatNo"
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:layout_gravity="center_vertical"
	                   android:paddingLeft="6.0dip"
	                   android:text="@string/empty"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	               <TextView android:id="@+id/select_seatNo_img"
	                   android:drawableRight="@drawable/btn_expand_background"
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:layout_gravity="center_vertical"
	                   android:paddingLeft="6.0dip"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	           </LinearLayout>
			   <LinearLayout
			       android:id="@+id/currentNoLayout"
	               android:layout_width="0dip"
	               android:layout_height="50dip"
	               android:layout_weight="1.0"
	               android:orientation="horizontal"
	               android:layout_gravity="center_vertical"
	               android:paddingLeft="10.0dip" >
	
	               <TextView
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:text="@string/current_number"
	                   android:layout_gravity="center_vertical"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	
	               <TextView
	                   android:id="@+id/currentNumber"
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:layout_gravity="center_vertical"
	                   android:paddingLeft="6.0dip"
	                   android:text="@string/empty"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	           </LinearLayout>
			</LinearLayout>
			<LinearLayout
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:gravity="center_vertical"
               android:baselineAligned="false"
               android:orientation="horizontal" >
	           <LinearLayout
	               android:id="@+id/myNoLayout"
	               android:layout_width="fill_parent"
	               android:layout_height="wrap_content"
	               android:gravity="center_vertical"
	               android:orientation="horizontal"
	               android:layout_weight="1.0"
	               android:padding="10.0dip"
	               android:visibility="gone" >
	
	               <TextView
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:text="@string/my_number"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	
	               <TextView
	                   android:id="@+id/myNumber"
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:paddingLeft="6.0dip"
	                   android:text="@string/empty"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	           </LinearLayout>
	
	           <LinearLayout
	               android:id="@+id/beforeYouLayout"
	               android:layout_width="fill_parent"
	               android:layout_height="wrap_content"
	               android:gravity="center_vertical"
	               android:orientation="horizontal"
	               android:layout_weight="1.0"
	               android:padding="10.0dip"
	               android:visibility="gone" >
	
	               <TextView
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:text="@string/before_you"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	
	               <TextView
	                   android:id="@+id/beforeYou"
	                   android:layout_width="wrap_content"
	                   android:layout_height="wrap_content"
	                   android:maxLines="2"
	                   android:paddingLeft="6.0dip"
	                   android:text="@string/empty"
	                   android:textAppearance="?android:textAppearanceMedium"
	                   android:textColor="#ff000000"
	                   android:textSize="16.0sp" />
	           </LinearLayout>
           </LinearLayout>
           </LinearLayout>
           <LinearLayout
               android:id="@+id/btn_GetNumberLayout"
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:gravity="center_vertical"
               android:orientation="horizontal"
               android:padding="10.0dip" >

               <Button
                   android:id="@+id/btn_GetNumber"
                   android:layout_width="fill_parent"
                   android:layout_height="35dip"
                   android:layout_gravity="center_vertical|center_vertical"
                   android:background="@drawable/btn_weight"
                   android:text="@string/nahao"
                   android:textAppearance="?android:textAppearanceMediumInverse"
                   android:textColor="#ff000000"
                   android:textSize="14.0sp" />
           </LinearLayout>
       
</LinearLayout>